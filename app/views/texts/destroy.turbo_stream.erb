<turbo-stream action="remove" target="text_<%= @text.id %>"></turbo-stream>
<turbo-stream action="prepend" target="container">
  <template>
    <div class="alert alert-success" style="margin-top: 1rem;">
      Text "<%= @text.title %>" was successfully deleted.
    </div>
  </template>
</turbo-stream>
